! $HOME/.Xresources

!Xft.dpi:		96
Xft.antialias:		True
Xft.autohint:		False
Xft.hinting:		True
Xft.hintstyle:		hintslight
Xft.rgba:		rgba
!Xft.lcdfilter:		lcddefault

Xcursor.theme:		Neutral_Plus

! ---| colors |--- {{{1
!#include "/home/racoon/.themes/terminal_colors/rcn.xcolors"
#include "/home/racoon/.themes/terminal_colors/base16-default.dark.256.xresources"

! ---| urxvt |--- {{{1
URxvt*depth:                32
URxvt*borderWidth:          0
URxvt*buffered:             True
URxvt*cursorBlink:          False
URxvt*cursorColor:          #d8d8d8
URxvt*font:                 xft:Terminess Powerline:style=regular:size=8
URxvt*boldFont:             xft:Terminess Powerline:style=bold:size=8
URxvt*italicFont:           xft:Terminess Powerline:style=italic:size=8
URxvt*boldItalicFont:       xft:Terminess Powerline:style=bolditalic:size=8
URxvt*intensityStyles:      False
URxvt*inheritPixmap:        False
URxvt*internalBorder:       4
URxvt*jumpScroll:           True
URxvt*letterSpace:          1
URxvt*saveLines:            65535
URxvt*scrollBar:            False
URxvt*scrollstyle:          plain
URxvt*scrollTtyKeypress:    True
URxvt*scrollTtyOutput:      False
URxvt*scrollwithbuffer:     False
URxvt*secondaryScroll:      True
URxvt*skipScroll:           True
URxvt*termName:             rxvt-unicode-256color
URxvt*urgentOnBell:         True
URxvt*visualBell:           False
URxvt.iso14755_52:          False
URxvt.iso14755:             False

! perl extensions & options
URxvt*perl-ext-common:      default,matcher,searchable-scrollback
URxvt*url-launcher:         firefox
URxvt*matcher.button:       1
URxvt*matcher.pattern.0:    \\b(?:(?:https?|ftp):\/\/|mailto:)[\\w\-\@;\/?:&=%\$_.+!*\x27(),~#]+ [\\w\-\@;\/?:&=%\$_+!*\x27()~]
URxvt*matcher.pattern.1:    \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.pattern.2:    \\bhttp:\/\/(www.youtube|youtube).com\/watch\\?v=([\\w-]*)

