! $HOME/.Xdefaults

Xft.dpi:		96
Xft.antialias:		True
Xft.autohint:		False
Xft.hinting:		True
Xft.hintstyle:		hintslight
Xft.rgba:		rgb
Xft.lcdfilter:		lcddefault

Xcursor.theme:		Neutral_Plus

! ---| urxvt |--- {{{1
URxvt*depth:		24
URxvt*borderWidth:	0
URxvt*cursorBlink:	False
URxvt*cursorColor:	#d8d8d8
!URxvt*fading:		20
URxvt*font:		xft:Terminus:size=8
URxvt*boldFont:		xft:Terminus:style=bold:size=8
URxvt*italicFont:	xft:Terminus:style=italic:size=8
URxvt*boldItalicFont:	xft:Terminus:style=bolditalic:size=8
URxvt*inheritPixmap:	False
URxvt*internalBorder:	4
URxvt*jumpScroll:	True
URxvt*reverseVideo:	False
URxvt*saveLines:	65535
URxvt*scrollBar:	False
URxvt*scrollstyle:	plain
URxvt*scrollTtyKeypress:True
URxvt*scrollTtyOutput:	False
URxvt*scrollwithbuffer:	False
URxvt*secondaryScroll:	True
URxvt*termName:		rxvt-256color
URxvt*urgentOnBell: 	True

! Real transparency (with composite and xcompmgr)
!urxvtTrans*depth:      32
!urxvtTrans*background: rgba:0000/0000/0000/dddd
!urxvtTrans*tintColor:  grey

! perl extensions & options
URxvt*perl-ext-common:	default,matcher,searchable-scrollback
URxvt*urlLauncher:	firefox
URxvt*matcher.button:	1
URxvt*matcher.pattern.0: \\b(?:(?:https?|ftp):\/\/|mailto:)[\\w\-\@;\/?:&=%\$_.+!*\x27(),~#]+ [\\w\-\@;\/?:&=%\$_+!*\x27()~]
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.pattern.2: \\bhttp:\/\/(www.youtube|youtube).com\/watch\\?v=([\\w-]*)
URxvt*matcher.launcher.2: /home/racoon/files/scripts/youtube-dl
URxvt*tabbed.tabbar-fg:	7
URxvt*tabbed.tabbar-bg:	8
URxvt*tabbed.tab-fg:	15
URxvt*tabbed.tab-bg:	8

